<%= simple_form_for @company do |form| %>
  <%= form.input :name %>
  <%= form.input :cnpj %>
  <%= form.input :address1 %>
  <%= form.input :address2 %>
  <%= form.input :street_number %>
  <%= form.input :zip_code %>
  <%= form.input :city %>
  <%= form.input :state %>
  <%= form.submit %>
<% end %>
